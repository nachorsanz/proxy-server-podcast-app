{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "vercel-sapper"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/get-top-podcast/(.*)",
      "dest": "/get-top-podcast"
    },
    {
      "src": "/get-podcast/(.*)",
      "dest": "/get-podcast"
    }
  ]
}
